{
  "name": "minesweeper-angular",
  "version": "0.1.0",
  "devDependencies": {
    "browserify": "10.2.3",
    "chai": "^3.0.0",
    "coffee-coverage": "^0.6.0",
    "coffee-script": "1.9.3",
    "del": "1.2.0",
    "gulp": "3.8.11",
    "gulp-add-src": "0.2.0",
    "gulp-coffee": "2.1.2",
    "gulp-concat": "2.5.2",
    "gulp-live-server": "0.0.16",
    "gulp-load-plugins": "0.5.1",
    "gulp-rename": "1.2.0",
    "gulp-sass": "2.0.1",
    "gulp-sourcemaps": "1.5.2",
    "gulp-uglify": "1.2.0",
    "gulp-util": "2.2.14",
    "istanbul": "^0.3.15",
    "jsdom": "^3.1.2",
    "mocha": "^2.2.5",
    "through": "2.3.7",
    "vinyl-buffer": "1.0.0",
    "vinyl-paths": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  },
  "dependencies": {
    "object-assign": "2.0.0",
    "react": "0.13.3",
    "reflux": "0.2.7"
  },
  "scripts": {
    "test": "mocha --compilers coffee:coffee-script/register --recursive --require ./Spec/cs-coverage-loader.js -t 15000 -R spec Spec/Unit/; istanbul report;"
  },
  "jest": {
    "scriptPreprocessor": "Spec/preprocessor.js",
    "collectCoverage": true,
    "testFileExtensions": [
      "coffee",
      "litcoffee",
      "coffee.md",
      "js"
    ],
    "moduleFileExtensions": [
      "coffee",
      "litcoffee",
      "coffee.md",
      "js"
    ],
    "testPathIgnorePatterns": [
      "/_.*\\..*"
    ],
    "collectCoverageOnlyFrom": {
      "Resources/build/build.js": true
    },
    "testDirectoryName": "Spec/Unit"
  }
}
